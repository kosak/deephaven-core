<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>Deephaven.Dhe_NetClient</RootNamespace>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <PackageId>Deephaven.EnterpriseClient</PackageId>
    <Title>Client Library for Deephaven Enterprise</Title>
    <Version>20240517.494.0</Version>
    <Authors>Deephaven Authors</Authors>
    <Company>Deephaven Data Labs LLC</Company>
    <Description>A .NET library for accessing data from Deephaven Enterprise servers.</Description>
    <Copyright>Copyright (c) 2016-2025 Deephaven Data Labs and Patent Pending</Copyright>
    <PackageProjectUrl>https://deephaven.io/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/deephaven/deephaven-enterprise-client</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <NoWarn>1701;1702;1591;612</NoWarn>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <NoWarn>1701;1702;1591;612</NoWarn>
  </PropertyGroup>


  <ItemGroup>
    <Protobuf Include="..\..\..\..\iris\proto\*\src\main\proto\deephaven_enterprise\proto\*.proto" ProtoRoot="..\..\..\..\iris\proto\proto-auth-grpc\src\main\proto" AdditionalImportDirs="..\..\..\..\iris\proto\proto-auth\src\main\proto;..\..\..\..\iris\proto\proto-common\src\main\proto;..\..\..\..\iris\proto\proto-config-grpc\src\main\proto;..\..\..\..\iris\proto\proto-config\src\main\proto;..\..\..\..\iris\proto\proto-controller-grpc\src\main\proto;..\..\..\..\iris\proto\proto-controller\src\main\proto;..\..\..\..\iris\proto\proto-registry-grpc\src\main\proto;..\..\..\..\iris\proto\proto-registry\src\main\proto;..\..\..\..\iris\proto\proto-routing-grpc\src\main\proto;..\..\..\..\iris\proto\proto-routing\src\main\proto;..\..\..\..\iris\proto\proto-schema-grpc\src\main\proto;..\..\..\..\iris\proto\proto-schema\src\main\proto;..\..\..\..\iris\proto\proto-serviceregistry-grpc\src\main\proto;..\..\..\..\iris\proto\proto-serviceregistry\src\main\proto;..\..\..\..\iris\proto\proto-unified-grpc\src\main\proto;..\..\..\..\iris\proto\proto-unified\src\main\proto" GrpcServices="Client" />
  </ItemGroup>


  <ItemGroup>
    <PackageReference Include="Deephaven.CommunityClient" Version="0.39.1-SNAPSHOT" />
    <PackageReference Include="Google.Protobuf" Version="3.31.1" />
    <PackageReference Include="Grpc.Net.Client" Version="2.71.0" />
    <PackageReference Include="Grpc.Tools" Version="2.72.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

</Project>
